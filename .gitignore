# Ignore directories generated by Composer
/cms/vendor/
/cms/web/core/
/cms/web/modules/contrib/
/cms/web/themes/contrib/
/cms/web/profiles/contrib/
/cms/web/libraries/

# Ignore Drupal's file directories.
/cms/web/sites/*/files/
/cms/web/sites/*/private/

# Ignore local development files.
/cms/web/sites/*/settings.local.php
/cms/web/sites/*/development.services.yml

# Ignore SimpleTest multi-site environment.
/cms/web/sites/simpletest

# Ignore dependency directories
/node_modules/

# Ignore temporary files
*.swp
*.swo
*~
*.bak
*.orig
*.patch
*.rej

# Ignore OS-generated files
.DS_Store
.Thumbs.db
.cache
.project
.settings
.idea
.vscode
